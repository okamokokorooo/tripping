.home_view
  .home_header
    = render_breadcrumbs separator: ' > ',class: "breadcrumbs"
    .title
      = link_to "TRIPPING", root_path, id:"titling"
    .logout
      = link_to 'ログアウト', destroy_user_session_path, method: :delete
    .logoutbtn

    .user_page
      = link_to users_path do
        = fa_icon 'user', class: 'icon'
  .main_view
    .genres
      - @genres.each do |genre|
        .genre
          .genre__name
            = link_to "#{genre.name}", genre_programs_path(genre.id)